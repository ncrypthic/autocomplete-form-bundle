<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="ris_autocomplete_form.autocomplete.class">LLA\AutocompleteFormBundle\Form\Type\Autocomplete\AutocompleteType</parameter>
    </parameters>
    <services>
        <service id="ris_autocomplete_form.autocomplete" 
            class="%ris_autocomplete_form.autocomplete.class%"
            public="true">
            <argument type="service" id="doctrine"/>
            <tag name="form.type" alias="autocomplete"/>
        </service>
    </services>
</container>
